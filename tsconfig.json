  "compilerOptions": {
    "module": "commonjs",
    "allowJs": true,
    "strict": true,
    "noImplicitAny": true,
    "lib": ["es2015"],
    "esModuleInterop": true,
    "target": "es6",
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist"
  }
}
